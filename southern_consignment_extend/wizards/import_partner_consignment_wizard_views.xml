<odoo>
  <record id="view_excel_import_wizard_form" model="ir.ui.view">
    <field name="name">import.partner.consignment.wizard.form</field>
    <field name="model">import.partner.consignment.wizard</field>
    <field name="arch" type="xml">
      <form string="Upload Excel File">
        <header>
          <p>Upload a spreadsheet of master records, the first column should contain either the product code or name and the second column should contain the quantity.</p>
        </header>
        <group>
          <field name="file" filename="filename"/>
          <field name="filename" invisible="True"/>
        </group>
        <footer>
          <button name="action_import_excel" string="Import Records" type="object" class="btn-primary"/>
          <button string="Cancel" special="cancel"/>
        </footer>
      </form>
    </field>
  </record>
  <record id="action_excel_import_wizard" model="ir.actions.act_window">
    <field name="name">Master Record Import</field>
    <field name="res_model">import.partner.consignment.wizard</field>
    <field name="view_mode">form</field>
    <field name="target">new</field>
  </record>
</odoo>